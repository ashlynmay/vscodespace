{
    "artificialDelayMilliseconds": 700,
    "keepExistingTerminalsOpen": false,
    "runOnStartup": true,
    "terminals": [
      {
        "splitTerminals": [
          {
            "name": "server",
            "commands": ["npm i", "npm run dev"]
          },
          {
            "name": "client",
            "commands": ["npm run dev:client"]
          },
          {
            "name": "test",
            "commands": ["jest --watch"]
          }
        ]
      }
    ]
  }